-- Put this in StarterPlayer > StarterPlayerScripts or under a ScreenGui

-- Create the UI
local player = game.Players.LocalPlayer
local playerGui = player:WaitForChild("PlayerGui")

local screenGui = Instance.new("ScreenGui")
screenGui.Name = "StylishLoadstringGUI"
screenGui.ResetOnSpawn = false
screenGui.Parent = playerGui

local frame = Instance.new("Frame")
frame.Size = UDim2.new(0, 300, 0, 200)
frame.Position = UDim2.new(0.5, -150, 0.5, -100)
frame.BackgroundColor3 = Color3.fromRGB(20, 40, 80)
frame.BorderSizePixel = 0
frame.Active = true
frame.Draggable = true
frame.Parent = screenGui

local originalSize = frame.Size
local originalPosition = frame.Position

-- Title Bar
local titleBar = Instance.new("Frame")
titleBar.Size = UDim2.new(1, 0, 0, 30)
titleBar.BackgroundColor3 = Color3.fromRGB(10, 25, 50)
titleBar.BorderSizePixel = 0
titleBar.Parent = frame

local titleLabel = Instance.new("TextLabel")
titleLabel.Size = UDim2.new(1, -60, 1, 0)
titleLabel.Position = UDim2.new(0, 10, 0, 0)
titleLabel.Text = "Stylish Executor"
titleLabel.TextColor3 = Color3.fromRGB(255, 255, 255)
titleLabel.TextXAlignment = Enum.TextXAlignment.Left
titleLabel.Font = Enum.Font.GothamBold
titleLabel.TextSize = 16
titleLabel.BackgroundTransparency = 1
titleLabel.Parent = titleBar

-- Minimize Button
local minimizeButton = Instance.new("TextButton")
minimizeButton.Size = UDim2.new(0, 50, 1, 0)
minimizeButton.Position = UDim2.new(1, -50, 0, 0)
minimizeButton.Text = "-"
minimizeButton.Font = Enum.Font.GothamBold
minimizeButton.TextSize = 18
minimizeButton.TextColor3 = Color3.fromRGB(255, 255, 255)
minimizeButton.BackgroundColor3 = Color3.fromRGB(15, 30, 60)
minimizeButton.BorderSizePixel = 0
minimizeButton.Parent = titleBar

-- Body
local bodyFrame = Instance.new("Frame")
bodyFrame.Name = "BodyContent"
bodyFrame.Size = UDim2.new(1, 0, 1, -30)
bodyFrame.Position = UDim2.new(0, 0, 0, 30)
bodyFrame.BackgroundTransparency = 1
bodyFrame.Parent = frame

-- Tabs
local tabFrame = Instance.new("Frame")
tabFrame.Size = UDim2.new(1, 0, 0, 40)
tabFrame.Position = UDim2.new(0, 0, 0, 0)
tabFrame.BackgroundColor3 = Color3.fromRGB(10, 25, 50)
tabFrame.Parent = bodyFrame

local function createTabButton(text, position)
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(0.33, 0, 1, 0)
	btn.Position = position
	btn.Text = text
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 16
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.BackgroundColor3 = Color3.fromRGB(30, 60, 120)
	btn.BorderSizePixel = 0
	btn.Parent = tabFrame
	return btn
end

local tabButton1 = createTabButton("For Mic Up", UDim2.new(0, 0, 0, 0))
local tabButton2 = createTabButton("Universal", UDim2.new(0.33, 0, 0, 0))
local tabButton3 = createTabButton("Other Tab", UDim2.new(0.66, 0, 0, 0))

local function createScrollFrame(parent)
	local scroll = Instance.new("ScrollingFrame")
	scroll.Size = UDim2.new(1, 0, 1, -40)
	scroll.Position = UDim2.new(0, 0, 0, 40)
	scroll.BackgroundColor3 = Color3.fromRGB(40, 60, 100)
	scroll.ScrollBarThickness = 10
	scroll.VerticalScrollBarPosition = Enum.VerticalScrollBarPosition.Right
	scroll.Visible = false
	scroll.Parent = parent

	local layout = Instance.new("UIListLayout")
	layout.FillDirection = Enum.FillDirection.Vertical
	layout.Padding = UDim.new(0, 10)
	layout.SortOrder = Enum.SortOrder.LayoutOrder
	layout.Parent = scroll

	return scroll
end

local scrollFrameForMicUp = createScrollFrame(bodyFrame)
local scrollFrameUniversal = createScrollFrame(bodyFrame)
local scrollFrameOtherTab = createScrollFrame(bodyFrame)

-- Create Buttons
local function createButton(text, parent, callback)
	local btn = Instance.new("TextButton")
	btn.Size = UDim2.new(0.8, 0, 0, 40)
	btn.Text = text
	btn.Font = Enum.Font.GothamBold
	btn.TextSize = 18
	btn.TextColor3 = Color3.fromRGB(255, 255, 255)
	btn.BackgroundColor3 = Color3.fromRGB(30, 60, 120)
	btn.BorderSizePixel = 0
	btn.Parent = parent
	btn.MouseButton1Click:Connect(callback)
end

-- Code Snippets
local micUpCode = [[repeat task.wait() until game.Players.LocalPlayer
loadstring(game:HttpGet("https://raw.githubusercontent.com/0riginalWarrior/Stalkie/refs/heads/main/roblox.lua"))()]]

local antiVoidCode = [[loadstring(game:HttpGet(('https://raw.githubusercontent.com/SlammedByCleetus/antivoid-for-mic-up-Roblox/refs/heads/main/AntiVoid.lua')))()]]

local unsuspendVoiceChatCode = [[loadstring(game:HttpGet('https://raw.githubusercontent.com/SlammedByCleetus/antivoid-for-mic-up-Roblox/refs/heads/main/unsuspend%20voicechat'))()]]

local infiniteYieldCode = [[loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()]]

local systemBrokenCode = [[loadstring(game:HttpGet("https://raw.githubusercontent.com/H20CalibreYT/SystemBroken/main/script"))()]]

-- Create Buttons
createButton("Execute For Mic Up", scrollFrameForMicUp, function()
	loadstring(micUpCode)()
end)

createButton("Execute AntiVoid", scrollFrameForMicUp, function()
	loadstring(antiVoidCode)()
end)

createButton("Unsuspend Voice Chat", scrollFrameUniversal, function()
	loadstring(unsuspendVoiceChatCode)()
end)

createButton("Infinite Yield", scrollFrameOtherTab, function()
	loadstring(infiniteYieldCode)()
end)

createButton("System Broken", scrollFrameOtherTab, function()
	loadstring(systemBrokenCode)()
end)

-- Tab Switch Logic
local function switchTab(tab)
	scrollFrameForMicUp.Visible = false
	scrollFrameUniversal.Visible = false
	scrollFrameOtherTab.Visible = false

	if tab == 1 then
		scrollFrameForMicUp.Visible = true
	elseif tab == 2 then
		scrollFrameUniversal.Visible = true
	elseif tab == 3 then
		scrollFrameOtherTab.Visible = true
	end
end

tabButton1.MouseButton1Click:Connect(function() switchTab(1) end)
tabButton2.MouseButton1Click:Connect(function() switchTab(2) end)
tabButton3.MouseButton1Click:Connect(function() switchTab(3) end)

switchTab(1)

-- Minimize Logic
local isMinimized = false

minimizeButton.MouseButton1Click:Connect(function()
	isMinimized = not isMinimized
	for _, child in ipairs(frame:GetChildren()) do
		if child ~= titleBar then
			child.Visible = not isMinimized
		end
	end
	if isMinimized then
		frame:TweenSize(UDim2.new(0, 300, 0, 30), "Out", "Sine", 0.3, true)
		frame:TweenPosition(UDim2.new(0.5, -150, 0, 0), "Out", "Sine", 0.3, true)
		minimizeButton.Text = "+"
	else
		frame:TweenSize(originalSize, "Out", "Sine", 0.3, true)
		frame:TweenPosition(originalPosition, "Out", "Sine", 0.3, true)
		minimizeButton.Text = "-"
	end
end)
